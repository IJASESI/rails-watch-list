<h1>Movie Lists</h1>
<ul class="list-group">
  <% @lists.each do |list| %>
    <li class="list-group-item d-flex justify-content-between align-items-center">
      <%= link_to list.name, list_path(list)%>
    </li>
  <% end %>
</ul>
<%= link_to "Create a new list", new_list_path, class: "btn btn-primary" %>
